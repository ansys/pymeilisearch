{
    "index_uid": "{{ index_uid }}",
    "start_urls": {{ start_url|safe }},
    "stop_urls": {{ stop_url|safe }},
    "selectors": {
        "lvl0": {
            "selector": "title",
	    "global": true,
            "default_value": "Documentation"
        },
        "lvl1": ".bd-content h1",
        "lvl2": ".bd-content h2",
        "lvl3": ".bd-content h3",
        "lvl4": ".bd-content h4",
        "lvl5": ".bd-content h5",
        "lvl6": ".bd-content h6",
        "text": ".bd-content p, .bd-content li, .bd-content td"
    },
    "strip_chars": " .,;:#",
    "selectors_exclude": [".bd-sidebar-primary", ".bd-sidebar-secondary"]
  }